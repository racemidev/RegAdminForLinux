#! /usr/bin/env python

from ef_utils import *

def my_test(self, d, test_dir, **kwds):
    new_key = "%s\\foo" % d[0]
    ef_debug_out("Adding ", new_key)
    self.assertGoodProg(CL_PROG, test_dir, "add_key", new_key)
    self.assertGoodProg(CL_PROG, test_dir, "has_key", new_key)
    self.assertGoodProg(DIFF_PROG, kwds['orig_dir'], test_dir)
    return

class RraRDAddKey2Test(EfTest):
    pass

add_key_example_tests(my_test, RraRDAddKey2Test)

if __name__ == '__main__':
    unittest.main()
