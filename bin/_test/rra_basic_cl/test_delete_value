#! /usr/bin/env python

from ef_utils import *

def my_test(self, d, test_dir, **kwds):
    ef_debug_out("Deleting '%s' from '%s'" % (d[1], d[0]))
    self.assertGoodProg(CL_PROG, test_dir, "delete_value", d[0], d[1])
    self.assertBadProg(CL_PROG, test_dir, "has_value", d[0], d[1])
    return

class DeleteValueTest(EfTest):
    pass

add_value_example_tests(my_test, DeleteValueTest)

if __name__ == '__main__':
    unittest.main()
